<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <title>Liste365</title>
</head>

<body>
  <div style="background-color: #005F76;">
    <nav class="navbar">
      <div class="container-fluid">
        <a class="navbar-brand" style="color: black;">
          <img src="/img/omega-365-logo.svg" alt="" class="d-inline-block align-text-center">
          Liste365
        </a>
      </div>
    </nav>
  </div>

  <ul>
    <% if(messages.reg_msg){%>
      <li>
        <%= messages.reg_msg %>
      </li>
      <%}%>
  </ul>

  <ul>
    <% if(messages.error){%>
      <li>
        <%= messages.error %>
      </li>
      <% }%>
  </ul>

  <form action="/login" method="POST">
    <div class="w-25 p-3">
      <label for="epost" class="form-label">Epost</label>
      <input type="email" class="form-control" name="epost" id="epost" required>
    </div>

    <div class="w-25 p-3">
      <label for="pass" class="form-label">Passord</label>
      <input type="password" class="form-control" name="pass" id="pass" required>

    </div>
    <div class="btn-group" style="padding-left: 1em;">
      <button type="submit" class="btn btn-primary">Login</button>
      <button type="button" class="btn btn-secondary" onclick="window.location.href='registrer'">Registrer</button>
    </div>
  </form>





  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
    crossorigin="anonymous"></script>

</body>

</html>