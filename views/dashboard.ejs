<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
  <title>Liste365</title>
</head>

<body>

  <div style="background-color: #005F76;">
    <nav class="navbar">
      <div class="container-fluid">
        <a class="navbar-brand" href="#" style="color: black;">
          <img src="/img/omega-365-logo.svg" alt="" class="d-inline-block align-text-center">
          Liste365
        </a>
        <button type="button" class="btn btn-primary" onclick="window.location.href='logut'">Logg av</button> 
      </div>
    </nav>
  </div>
  <h1 style="padding-left: 1em;">Velkommen <%= brukernavn %> </h1>

  <div class="m-4">

    <div class="list-group w-50" style="padding-bottom: 1em;">
      <% if(listeNavn.length > 0){%>
      <% listeNavn.forEach(liste => {%>
        <a href="#" class="list-group-item list-group-item-action">
          <i class="bi bi-card-list" style="padding-right: 1em;"></i><%= liste%> </a>
        <%})%><%}%>      
    </div>

    <div class="w-25">  
      <form action="" method="post">
        <label for="navn">Ny Liste</label>
        <input type="name " class="form-control" name="ListeNavn" id="ListeNavn" placeholder="Liste Navn" required>
        <div style="padding-top: 0.5em;">
          <input class="btn btn-primary" type="submit" name="newList" value="Lag Liste" />
        </div>
      </form>
    </div>

</div>
  







  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
    crossorigin="anonymous"></script>

</body>

</html>